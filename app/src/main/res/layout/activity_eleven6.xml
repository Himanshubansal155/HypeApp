<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.rc.myapplication.Eleven6"
    android:background="@drawable/quiz6">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:textAlignment="center"
        android:id="@+id/textView18"
        android:textSize="30dp"
        android:textColor="#ffeccf"
        android:layout_x="47dp"
        android:layout_y="195dp"
        android:layout_above="@+id/textView7"
        android:layout_marginBottom="65dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Thank You For Participation"
        android:textAlignment="center"
        android:textSize="30dp"
        android:textColor="#fff1dd"
        android:layout_x="114dp"
        android:layout_y="94dp"
        android:id="@+id/textView8"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp" />

    <Button
        style="@style/Base.Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GO to menu"
        android:layout_x="190dp"
        android:layout_y="305dp"
        android:id="@+id/button35"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/button36"
        android:layout_centerHorizontal="true"
        android:textColor="#edecec" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Share your score"
        android:layout_x="91dp"
        android:layout_y="257dp"
        android:textSize="30dp"
        android:id="@+id/textView7"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:textColor="#000000" />

    <Button
        style="@style/Base.Widget.AppCompat.Button.Borderless"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button38"
        android:layout_x="21dp"
        android:layout_y="264dp"
        android:background="@drawable/ic_share"
        android:layout_below="@+id/textView7"
        android:layout_alignLeft="@+id/button35"
        android:layout_alignStart="@+id/button35" />

    <Button
        style="@style/Base.Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Check Your Answers"
        android:id="@+id/button36"
        android:textColor="#edecec"
        android:layout_below="@+id/button38"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp" />

</RelativeLayout>
